.header p{
	color:#fff;
	font-size: 36px;
}
.logo{
	width:100%;
	height: 166px;
	margin-top:156px;	
	margin-bottom: 40px;	
	text-align: center;
	
}
.logo_pic{
	border-radius: 166px;
	height: 166px;
	width:166px;
	margin: 0 auto;
}
#login {
	width:720px;
		line-height: 96px;
}
#login div {
	font-size:32px ;
	line-height: 96px;
	width:600px;
	margin:0 auto;
	background: #fff;	
	margin-bottom: 20px;
	border:1px solid #C6C6C6;
}

#login div input {
	width: 540px;
	line-height: 32px;
	padding: 0;
	border:none;
	font-size: 32px;
	margin-left: 30px;
}
#login div input text{
		line-height: 94px;
}
.btnLogin {
	width: 600px;
	height: 72px;
	margin:0 auto;
	line-height: 72px;
	border-radius: 5px;
	text-align: center;
	color: #fff;
	background: #33B5E6;
	border: 0;
	margin-top:40px;
	margin-bottom: 40px;
	display: block;
	font-size: 36px;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
#login a {
	width: 360px;
	height:32px;
	line-height: 32px;
	color: #33B5E6;
	float:left;
	font-size: 32px;
    text-align: center;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}

