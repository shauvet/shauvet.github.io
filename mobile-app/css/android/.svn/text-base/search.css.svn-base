}
.block_pub_title {
	margin: 0 auto;
	width: 660px;
	line-height: 72px;
	font-size: 32px;
	color: #000;
	font-weight: bold;
}
.block_img {
	margin: 0 auto;
	width: 660px;
	height: 120px;
	margin-bottom: 15px;
}
.first_img {
	width: 120px;
	height: 120px;
	float: left;
	background: #B7B7B7;
}
.after_img {
