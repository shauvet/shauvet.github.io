}
.detail_xian {
	width: 80%;
	margin-left: 15%;
	border-bottom: 1px solid #d9d9d9;
}
.block {
	width: 100%;
	background: #fff;
	margin-bottom: 10px;
	border-bottom: 1px solid #ddd;
}
.block_title {
	width: 100%;
	height: 96px;
	line-height: 96px;
}
.block_title img {
	height: 48px;
	width: 48px;
	margin-left: 30px;
	margin-top: 24px;
	float: left;
}
.block_title span {
	margin-left: 30px;
	font-size: 36px;
	color: #000;
}
.block1 {
	width: 100%;
	height: 96px;
	background: #fff;
}
.block_pub {
	width: 100%;
	background: #fff;
}
.block1 span {
	float: left;
}
.name {
	line-height: 96px;
	margin-left: 30px;
	font-size: 32px;
	color: #FE0000;
	font-weight: bold;
	float: left;
}
.dep {
	font-size: 20px;
	color: #B4B4B4;
	height: 96px;
	line-height: 96px;
	padding-top: 5px;
	margin-left: 20px;
	float: left;
}
.phone {
	font-size: 32px;
	color: #000;
	height: 96px;
	line-height: 96px;
	float: left;
	margin-left: 30px;
}
.company {
	line-height: 92px;
	margin-left: 30px;
	font-size: 32px;
	color: #000;
