	height: 120px;
	float: left;
	margin-left: 10px;
	background: #B7B7B7;
}
.block_time {
	width: 660px;
	line-height: 34px;
	color: #696969;
	font-size: 24px;
	margin: 0 auto;
}
.block_address {
	width: 660px;
	line-height: 34px;
	margin: 0 auto;
	color: #696969;
	font-size: 24px;
}
.block_address span {
	margin-left: 10px;
}
.yuyin {
	width: 660px;
	line-height: 34px;
	margin: 0 auto;
	margin-bottom: 40px;
}
.yuyin a {
	font-size: 24px;
	color: #33B5E7;
}
/** 详细页面 */
.custom-radio {
	width: 100%;
	height: 72px;
	line-height: 72px;
	background: #fff;
	padding-left: 44px;
	font-size: 32px;
	color: #000;
}
.custom-radio label {
	background: url(../../images/btn_unselect.png) no-repeat 0 5px;
}
.custom-radio label.checked {
	background: url(../../images/btn_selcet.png) no-repeat 0 5px;
}
.custom-radio input {
	position: relative;
	left:20px;
    display: none;
} 
.custom-radio label {
	padding-left:60px;
}
.save{
	float: left;
	width:96px;
	height:96px;
}
.savebtn{
	width: 96px;
	height: 96px;
	line-height: 96px;
	letter-spacing:3px;
	color: #fff;
	background: #33B5E6;
	border: none;
	font-size: 32px;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.text_input{
	width:100%;
	height:96px;
	line-height: 96px;
	background: #fff;
	border-top: 1px solid #ddd;
	border-bottom: 1px solid #ddd;
}
.text_input input{
	border:none;
	line-height: 40px;
	width:672px;
	font-size:32px;
	color:#000;
	margin-left:24px;
	margin-top: 20px;
}
.text_textarea{
	width:100%;
	background: #fff;
	border-top: 1px solid #ddd;
	border-bottom: 1px solid #ddd;
}
.text_textarea textarea{
	width:672px;
	height:400px; 
	font-size:32px;
	line-height: 50px;
	color:#000;
	border: none;
	margin-left:24px;
	resize:none;
}
.important_client{
	width:100%;
	height:96px;
	background: #fff;
	margin-top: 40px;
	border-top: 1px solid #ddd;
	border-bottom: 1px solid #ddd;
}
.highlight{
	font-size: 24px;
	float: left;
	width: 32px;
	height: 32px;
	color: #fff;
	margin-left:24px;
	background: #FE0000;
	line-height: 32px;
	margin-top: 32px;
	text-align: center;
	-moz-border-radius: 5px; 
	-webkit-border-radius: 5px;  
	border-radius: 5px;
}
.important{
	float: left;
	color:#000;
	line-height: 96px;
	font-size:32px;
	margin-left:15px;
}
.mui-switch{
	width:80px;  
	height:40px;
	float:right;
	margin-right:24px;
	margin-top: 28px;
}
.mui-switch.mui-active {
	border-color: #4cd964;
	background-color: #4cd964
}
.mui-switch:before{
	top:0;
	line-height:40px; 
	color:#000
} 
.mui-switch.mui-active:before{  
	line-height:40px;
	color:#fff
}
.mui-switch .mui-switch-handle{
	height:38px;
	width:38px;
	left:0;
	border-radius: 40px;
}
