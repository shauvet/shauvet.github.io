/** header */

.header {
	height: 96px;
	line-height: 96px;
	width: 100%;
	top: 0px;
	background: #33B5E6;
	position: fixed;
	z-index: 5;
	text-align: center;
}
.back {
	width:96px;
	height: 96px;
	line-height: 96px;
	float: left;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.header a {
	width: 96px;
	height: 96px;
	line-height: 96px;
	float: left;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.header a img {
	padding-top: 30px;
}
.header_middle{
	width:430px;
	line-height: 96px;
	float: left;
}

/** 弹出窗 */
.news {
	display: none;
	width: 260px;
	z-index: 1050;
	text-align: center;
	position: absolute;
	top:96px;
	right: 112px;
}
.news_list {
	text-align: center;
	background: #fff;
	border-bottom: 1px solid #d9d9d9;
	width: 260px;
	line-height: 72px;
	font-size: 32px;
	color: #333;
}
.news_list_top{
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}
.news_list_bottom{
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
}
.modal-backdrop {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 1040;
	background-color: #000;
	opacity: 0.2;
	filter: alpha(opacity=60)
}
/** 下拉选项 */
select {
	display: none;
}
.select_box {
	z-index: 6;
	font-size: 36px;
	text-align: center;
	line-height: 96px;
	color: #fff;
	float: left;
	width: 330px;
	margin-left: 80px;	
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.select_showbox {
	width: 330px;
	height: 96px;
	background: #32B5E3 url(../../images/btn_wodexia.png) no-repeat 260px 35px;
}
.select_option {
	position: absolute;
	left: 0;
	width: 720px;
	border-top: 1px solid #ddd;
	display: none;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
.select_option li {
	list-style-type: none;
	background: #fff;
	border-bottom: 1px solid #ddd;
	color: #333
}
.select_option li.selected {
	background: #fff;
	color: #333;
}
.select_option li.hover {
	background: #fff;
	color: #333;
}
.select_check{
	position: relative;
	top:96px;
	width:100%;
	height:96px;
}
.check { 
	width: 50%;
	height: 96px;
	float: left;
	background: #fff;
	border-left: 1px solid #E0E0E0;
	border-right: 1px solid #E0E0E0;
	border-bottom: 2px solid #E0E0E0;
}
/* diy_select */

.diy_select {
	height: 96px;
	width: 100%;
	font-size: 36px;
	background: #fff;
	color: #000;
	float: left;
	border-bottom: 1px solid #ddd;
	text-align: center;
}
.diy_select_btn {
	text-align: left;
	line-height: 94px;
}
.diy_select_txt {
	float: left;
	height: 100%;
	line-height: 96px;
	margin-left: 10%;
}
.diy_select_btn img {
	margin-top: 25px;
}
.diy_select,
.diy_select_list {
	background: #fff;
}
.diy_select_txt {
	width: 60%;
}
.diy_select_txt,
.diy_select_list li {
	text-indent: 10px;
	overflow: hidden;
}
.diy_select_list {
	position: absolute;
	top: 2px;
	z-index: 10;
	width: 50%;
	display: none;
	margin-top: 96px;
}
.diy_select_list li {
	list-style: none;
	height: 96px;
	color:#000;
	line-height: 96px;
	cursor: default;
	border-bottom: 2px solid #E0E0E0;
}
.diy_select_list li a{
		color:#000;
}
.search {
	position: relative;
	top: 116px;
	width: 672px;
	background: #fff;
	margin:0 auto;
	height: 60px;
	line-height: 60px;
	border-radius: 10px;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
	box-shadow: 0 0 0 2px #fff;
}
.search a img{
	margin-top:5px;
	margin-left: 10px; 
	float: left;
}
input[type=search] {
	font-size: 24px;
	height: 30px;
	line-height: 30px;
	width: 540px;
	color: #909090;
	margin-top: 15px;
 	margin-left: 20px;
	border:none;
	float: left;
}
.rest{
	float: left;
	height:60px;
}
.rest img{
	margin-top:14px;
}
/**滚动条控制*/
.scroll_total{
	width:100%;
	position:absolute;  
	top:290px;   
	left:0;
	bottom:0;
	z-index: -1;
	overflow:auto; 	
}
/** 内容 */

.content {
	position: relative;
	width: 720px;
	background: #fff;
	z-index: 1;
	border-bottom: 1px solid #ddd;
}
.mui_add {
	position: static; 
	top: 0px;
	margin-top: 0; 
}
.left {
	float: left;
}
.clear {
	margin: 0;
	padding: 0;
	border: 0;
	clear: both;
	width: 100%;
	height: 40px;
}
.edit {
	width: 58px;
	text-align: right;
	position: absolute;
	bottom:43%;
	right:24px;
}
.title {
	width: 614px;
	margin-left: 24px;
	line-height: 76px;
}
.company {
	font-size: 32px;
	color: #2f336f;
	font-weight: bold;
}
.company p {
	float: right;
	font-size: 24px;
	color: #000;
	margin-right: 50px;
	font-weight: normal;
}
.content_name{
	line-height: 60px;
	font-size:32px;
	color:#2f336f;
	font-weight: bold;
}
.content_name span{
	font-size: 24px;
	color: #000;
	margin-right: 30px;
	font-weight: normal;
}
.content_name a{
	font-size: 32px;
	color:#2f336f;
	font-weight: normal;
}
.content_company{
	font-size: 24px;
	color: #696969;
	line-height: 30px;
	margin-left: 24px;
}
.content_company span{
	color:#33b5e4;
	font-size:20px;
	margin-left:10px;
}
.name {
	width: 614px;
	line-height: 30px;
	font-size: 28px;
	font-weight: bold;
	color: #909090;
	margin-left: 24px;
}
.name p {
	width: 614px;
	line-height: 25px;
}
.record {
	font-size: 28px;
	width: 614px;
	color: #909090;
	font-weight: bold;
	line-height: 50px;
	margin-left: 24px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.abstract {
	margin-left: 24px;
	width: 614px;
	line-height: 40px;
	font-size: 24px;
	color: #909090;
}
.abstract p {
	color: #33B5E6;
	font-size: 20px;
	float: left;
}
.abstract span {
	margin-right: 20px;
}